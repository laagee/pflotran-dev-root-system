#================================================
# Description: DFN with 2 intersecting fractures
# Test problem for richards equation
# Satish Karra, LANL
# Email: satkarra@lanl.gov
# April 08, 2013
#================================================

DFN

#========================== regression ========================================
REGRESSION
  CELLS
    1
    3
    5
    7
    9
    11
    13
    15
  /
END

#=========================== flow mode ========================================
MODE RICHARDS

#=========================== discretization ===================================
GRID
  TYPE unstructured_explicit dfn.uge
END

PRINT_PRIMAL_GRID

#=========================== fluid properties =================================
FLUID_PROPERTY
  DIFFUSION_COEFFICIENT 1.d-9
END

#=========================== material properties ==============================
MATERIAL_PROPERTY soil1
  ID 1
  POROSITY 0.25d0
  TORTUOSITY 0.5d0
  SATURATION_FUNCTION default
  PERMEABILITY
    PERM_ISO 1.d-12
  /
END

#=========================== saturation functions =============================
SATURATION_FUNCTION default
  SATURATION_FUNCTION_TYPE BROOKS_COREY
  RESIDUAL_SATURATION 0.1d0
  LAMBDA 0.3d0
  ALPHA 1.d-4
END

#=========================== output options ===================================
OUTPUT
  FORMAT TECPLOT BLOCK
  HDF5_WRITE_GROUP_SIZE 1
END

#=========================== times ============================================
TIME
  FINAL_TIME 100.d0 s
  MAXIMUM_TIMESTEP_SIZE 10.d0 d
END

REFERENCE_PRESSURE 101325.

#=========================== regions ==========================================
REGION All
  COORDINATES
    -1.d20 -1.d20 -1.d20
    1.d20 1.d20 1.d20
  /
END 


REGION Front
  FILE dfn_front.ex
END 

#=========================== flow conditions ==================================
FLOW_CONDITION initial
  TYPE
     PRESSURE hydrostatic
  /
  DATUM 0.d0 0.d0 1.d0
  PRESSURE 101325.d0
END


FLOW_CONDITION front
  TYPE 
     PRESSURE dirichlet
  /
  PRESSURE 2.d5
END

#=========================== condition couplers ===============================
# initial condition
INITIAL_CONDITION
  FLOW_CONDITION initial
  REGION All
END


BOUNDARY_CONDITION Front
  FLOW_CONDITION front
  REGION Front	
END


#=========================== stratigraphy couplers ============================
STRATA
  REGION All 
  MATERIAL soil1
END
